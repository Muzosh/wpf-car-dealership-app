<Window x:Class="BOOP_Project.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BOOP_Project"
        mc:Ignorable="d"
        Title="Testing" Height="450" Width="800">
    <Grid>
        <Button x:Name="doNothingButton" Content="DO NOTHING" HorizontalAlignment="Center" VerticalAlignment="Center"
                Width="330" Height="100" Cursor="Hand" Click="DoNothingButton_Click" FontSize="24" FontFamily="Comic Sans MS"/>
        <TextBox x:Name="inputTextBox" HorizontalAlignment="Left" Height="23"
            Margin="92,70,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="InputTextBox_TextChanged_1"/>
        <TextBox x:Name="resultTextBox" HorizontalAlignment="Left" Height="23"
                 Margin="324,70,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label" Content="x(252/pi) - 685 = " HorizontalAlignment="Left" Margin="217,70,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
